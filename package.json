{
  "name": "go-tooo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "pnpm@8.10.2",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:css\" \"pnpm run dev:go\" \"pnpm run dev:templates\"",
    "dev:css": "tailwindcss -i internal/assets/tailwind.css -o internal/assets/dist/style.css --watch",
    "dev:go": "nodemon --signal SIGTERM --watch '*/**' --ext go,css --exec \"go run main.go\"",
    "dev:templates": "nodemon --signal SIGTERM --watch '*/**' --ext templ --exec \"templ generate\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}